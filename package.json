    "express": "^4.18.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.13",
    "@babel/node": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "node-sass-middleware": "^1.0.1",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2"
  }
}
